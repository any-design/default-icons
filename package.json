{
  "name": "@any-design/default-icons",
  "version": "0.1.1",
  "description": "Default icon bundle for AnyUI",
  "main": "dist/default-icons.umd.js",
  "module": "dist/default-icons.esm.js",
  "typings": "dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "cross-env NODE_ENV=dev rollup -c rollup.config.ts -w",
    "build": "rimraf ./dist && tsc --module commonjs && cross-env NODE_ENV=production rollup -c rollup.config.ts && rimraf ./dist/lib",
    "lint": "eslint ./src/**/*"
  },
  "author": "BackRunner",
  "license": "MIT",
  "peerDependencies": {
    "@any-design/anyui": "^0.1.0"
  },
  "devDependencies": {
    "@iconify/icons-fluent": "^1.2.34",
    "@iconify/icons-ic": "^1.2.13",
    "@iconify/icons-ph": "^1.2.4",
    "@iconify/icons-quill": "^1.2.5",
    "@iconify/icons-si-glyph": "^1.2.6",
    "@iconify/types": "^2.0.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^18.17.16",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
